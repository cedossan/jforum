<html t:type="layout" title="Welcome"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

    <table t:type="grid" t:source="boards" t:row="board">
        <p:nameCell>
            <a t:type="pagelink" t:page="board" t:parameters="{'b': board.id }" href="#">${board.name}</a>
        </p:nameCell>
    </table>
</html>
